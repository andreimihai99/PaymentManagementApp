<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegisterAdminController">
   <children>
      <PasswordField fx:id="passwordField" layoutX="76.0" layoutY="240.0" promptText="password" />
      <TextField fx:id="emailField" layoutX="76.0" layoutY="200.0" promptText="email" />
      <TextField fx:id="surnameField" layoutX="76.0" layoutY="126.0" promptText="surname" />
      <TextField fx:id="nameField" layoutX="76.0" layoutY="89.0" promptText="name" />
      <TextField fx:id="keyField" layoutX="76.0" layoutY="274.0" promptText="key" />
      <Label layoutX="96.0" layoutY="40.0" text="Register Admin">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="registerButton" layoutX="120.0" layoutY="319.0" mnemonicParsing="false" onAction="#clickRegisterAdmin" text="Register" />
      <Button fx:id="backButton" layoutX="130.0" layoutY="351.0" mnemonicParsing="false" onAction="#clickBackAdmin" text="Back" />
      <TextField fx:id="adminUsernameField" layoutX="76.0" layoutY="161.0" promptText="username" />
   </children>
</AnchorPane>
